
# Supabase Configuration
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# Solana RPC URL (Use a fast provider like QuickNode, Alchemy, etc.)
RPC_URL=https://api.mainnet-beta.solana.com

# Your wallet address
WALLET_ADDRESS=YourSolanaWalletAddressHere

# Your private key in base58 format
# NEVER share your private key or commit it to version control
PRIVATE_KEY=YourBase58PrivateKeyHere

# Buy Conditions
MINIMUM_LIQUIDITY=1000.0
SLIPPAGE=1.0
ALLOWED_DEXES=jupiter,raydium
REQUIRE_VERIFIED_CONTRACT=true
MAX_PRIORITY_FEE=0.000005
ENABLE_ANTIBOT=true

# Sell Conditions
TARGET_PROFIT=20.0
STOP_LOSS=10.0
MAX_HOLDING_TIME=60
SELL_ON_VOLATILITY_SPIKE=false

# Risk Control
POSITION_SIZE_PERCENTAGE=5.0
MAX_OPEN_TRADES=3
COOLDOWN_PERIOD=30
SWAP_AMOUNT_USDC=10.0

# Optional: Telegram alerts configuration
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=

# Optional: Token filtering
# TOKEN_WHITELIST=address1,address2
# TOKEN_BLACKLIST=address1,address2

# Optional: Performance tuning
SCAN_INTERVAL=1.0
PRICE_CHECK_INTERVAL=5.0
